local player = game.Players.LocalPlayer local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))() local Window = OrionLib:MakeWindow({Name = "James Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"}) --[[ Name = - James Hub. HidePremium = - Whether or not the user details shows Premium status or not. SaveConfig = - Toggles the config saving in the UI. ConfigFolder = - The name of the folder where the configs are saved. IntroEnabled = false - Whether or not to show the intro animation. IntroText = - Text to show in the intro animation. IntroIcon = - URL to the image you want to use in the intro animation. Icon = - URL to the image you want displayed on the window. CloseCallback = - Function to execute when the window is closed. ]] local Tab = Window:MakeTab({ Name = "Speed Selection", Icon = "rbxassetid://4483345998", PremiumOnly = false }) --[[ Name = - The name of the tab. Icon = - The icon of the tab. PremiumOnly = - Makes the tab accessible to Sirus Premium users only. ]] local Section = Tab:AddSection({ Name = "Select Amount Speed" }) --[[ Name = - The name of the section. ]] OrionLib:MakeNotification({ Name = "WELCOME TO MY HUB!", Content = "Welcome To My Hub Wait To Load... ", Image = "rbxassetid://4483345998", Time = 10 }) --[[ Title = - The title of the notification. Content = - The content of the notification. Image = - The icon of the notification. Time = - The duration of the notfication. ]] Tab:AddButton({ Name = "SONIC (50%)", Callback = function() player.Character.Humanoid.WalkSpeed = 100 end }) --[[ Name = - The name of the button. Callback = - The function of the button. ]] Tab:AddButton({ Name = "FLASH (75%)", Callback = function() player.Character.Humanoid.WalkSpeed = 500 end }) --[[ Name = - The name of the button. Callback = - The function of the button. ]] Tab:AddButton({ Name = "SONIC+FLASH (100%)", Callback = function() player.Character.Humanoid.WalkSpeed = 3000 end }) --[[ Name = - The name of the button. Callback = - The function of the button. ]] --[[ Name = - The name of the button. Callback = - The function of the button. ]] local Tab = Window:MakeTab({
	Name = "Jump Selection",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]] local Section = Tab:AddSection({ Name = "Select Amount Jump" }) --[[ Name = - The name of the section. ]] OrionLib:MakeNotification({ Name = "WELCOME MY FRIENDS", Content = "Welcome To My Hub Done Load Play Safe Nigger ", Image = "rbxassetid://4483345998", Time = 10 }) --[[ Title = - The title of the notification. Content = - The content of the notification. Image = - The icon of the notification. Time = - The duration of the notfication. ]] Tab:AddButton({ Name = "SPIDERMAN (50%)", Callback = function() player.Character.Humanoid.JumpPower = 100 end }) --[[ Name = - The name of the button. Callback = - The function of the button. ]] Tab:AddButton({ Name = "HULK (75%)", Callback = function() player.Character.Humanoid.JumpPower = 300 end }) --[[ Name = - The name of the button. Callback = - The function of the button. ]] Tab:AddButton({ Name = "SUPERMAN (100%)", Callback = function() player.Character.Humanoid.JumpPower = 500 end }) --[[ Name = - The name of the button. Callback = - The function of the button. ]] --[[ Name = - The name of the button. Callback = - The function of the button. ]]
